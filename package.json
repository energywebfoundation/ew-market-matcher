{
  "name": "ewf-coo-matcher",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/test/*.ts",
    "start": "node dist/js/src/index.js",
    "debug": "node --inspect build/index.js",
    "build": "rm -rf dist/ && npm run generate-schemas && tsc ",
    "generate-schemas": "rm -rf schemas/ && mkdir schemas/ && npm run generate-sim-flow-schema && npm run generate-rule-schema && npm run generate-conf-schema",
    "generate-sim-flow-schema": "typescript-json-schema --required --ignoreErrors  tsconfig.json SimulationFlow > schemas/simulation-description.schema.json",
    "generate-rule-schema": "typescript-json-schema --required --ignoreErrors  tsconfig.json RuleConf > schemas/rule.schema.json",
    "generate-conf-schema": "typescript-json-schema --required --ignoreErrors  tsconfig.json MatcherConf > schemas/conf.schema.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slockit/ewf-coo-matcher.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/slockit/ewf-coo-matcher/issues"
  },
  "homepage": "https://github.com/slockit/ewf-coo-matcher#readme",
  "dependencies": {
    "@types/log-symbols": "^2.0.0",
    "@types/mathjs": "^3.21.0",
    "ethereumjs-tx": "^1.3.4",
    "ew-asset-registry-lib": "0.0.12-beta14",
    "ew-market-lib": "0.0.6-beta02",
    "ew-origin-lib": "0.0.2-beta7",
    "ew-utils-general-lib": "0.0.9-beta04",
    "jsonschema": "^1.2.4",
    "log-symbols": "^2.2.0",
    "numeric": "^1.2.6",
    "tslint": "^5.12.1",
    "underscore": "^1.9.1",
    "web3": "1.0.0-beta.37",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.4.5",
    "chai": "^4.1.2",
    "json-schema-to-typescript": "^5.5.0",
    "json2ts": "0.0.7",
    "mocha": "^5.0.5",
    "ts-node": "^5.0.1",
    "typescript": "^3.2.4",
    "typescript-json-schema": "^0.25.1"
  }
}
